<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
        <ui:style>
            .noSpace{
                margin-bottom: 0px;
                border-radius: 0px;
                z-index: 10;
            }
        </ui:style>
         <b:Navbar addStyleNames=" navbar-inverse {style.noSpace}">
            <b:NavbarHeader>
                <b:NavbarBrand>ASofCP(v.0.0.1)</b:NavbarBrand>
                <b:NavbarCollapseButton target="#navbar-collapse"/>
            </b:NavbarHeader>

             <b:NavbarNav ui:field="contextMenu"/>

             <b:NavbarCollapse b:id="navbar-collapse">
                <b:NavbarText pull="RIGHT">
                  <b:Icon type="USER"></b:Icon>
                  <b:NavbarLink>Administrator</b:NavbarLink>
                </b:NavbarText>
            </b:NavbarCollapse>

        </b:Navbar>


</ui:UiBinder>