<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="administrator">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="it.q02.asocp.modules.base.Base"/>
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="org.gwtbootstrap3.extras.select.Select"/>
    <inherits name="org.hibernate.validator.HibernateValidator" />

    <servlet path="/TicketRollService" class="it.q02.asocp.modules.administrator.server.rpc.TicketRollServiceImpl"/>
    <servlet path="/AttractionService" class="it.q02.asocp.modules.administrator.server.rpc.AttractionServiceImpl"/>

    <replace-with class="it.q02.asocp.modules.administrator.client.impl.ClientFactoryImpl">
        <when-type-is class="it.q02.asocp.modules.administrator.client.ClientFactory"/>
    </replace-with>
    <entry-point class='it.q02.asocp.modules.administrator.client.Administrator'/>
    <set-property name="user.agent" value="safari"/>
</module>