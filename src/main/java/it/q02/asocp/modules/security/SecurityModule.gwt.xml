<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="security">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
    <inherits name="it.q02.asocp.modules.base.Base"/>

    <entry-point class='it.q02.asocp.modules.security.client.SecurityModule'/>
    <servlet path="/CanHandleUserRoles" class="it.q02.asocp.modules.security.server.rpc.CanHandleUserRolesImpl"/>
    <set-property name="user.agent" value="safari"/>

</module>